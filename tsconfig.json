{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "rc-align": [
        "src/index.ts"
      ]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "**/*.ts", "**/*.tsx"]
}
